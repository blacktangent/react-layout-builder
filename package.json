{
    "private": true,
    "dependencies": {
        "react": "^15.4.x",
        "react-dom": "^15.4.x"
    },
    "scripts": {
        "setup": "yarn && lerna bootstrap",
        "test": "lerna run test"
    },
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-jest": "^20.0.3",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-react": "^6.24.1",
        "enzyme": "^2.9.1",
        "eslint": "^4.4.1",
        "eslint-plugin-react": "^7.2.1",
        "jest": "^20.0.4",
        "lerna": "^2.0.0",
        "react-test-renderer": "^15.6.1"
    },
    "version": "v0.27.5",
    "jest": {
        "bail": true,
        "automock": true,
        "verbose": true,
        "resetModules": true,
        "transform": {
            ".*": "<rootDir>/../../node_modules/babel-jest"
        },
        "roots": [
            "<rootDir>/__tests__"
        ],
        "testRegex": ".*-test.*",
        "unmockedModulePathPatterns": [
            "react",
            "node_modules"
        ],
        "modulePathIgnorePatterns": []
    }
}
