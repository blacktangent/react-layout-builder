{
  "name": "react-layout-builder",
  "version": "1.0.12",
  "description": "Build form layout",
  "main": "build/index.js",
  "author": "code@alvinsj.net",
  "repository": {
    "type": "git",
    "url": "https://github.com/blacktangent/react-layout-builder"
  },
  "keywords": [
    "reactjs",
    "form",
    "layout",
    "builder",
    "react-component"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/blacktangent/react-layout-builder/issues"
  },
  "homepage": "https://github.com/blacktangent/react-layout-builder",
  "peerDependencies": {
    "react": "^15.x.x"
  },
  "dependencies": {
    "object-assign": "^4.x.x",
    "form-input-serialize": "^1.0.12"
  },
  "scripts": {
    "jest": "NODE_PATH=src:NODE_PATH jest",
    "test": "jest",
    "jest:watch": "NODE_PATH=src:NODE_PATH jest --watch",
    "compile": "babel -d build/ src/",
    "prepublish": "babel -d build/ src/"
  },
  "jest": {
    "bail": true,
    "automock": true,
    "verbose": true,
    "resetModules": true,
    "transform": {".*": "<rootDir>/../../node_modules/babel-jest"},
    "roots": [
      "<rootDir>/__tests__"
    ],
    "testRegex": ".*-test.*",
    "testPathIgnorePatterns": [
      "pending"
    ],
    "unmockedModulePathPatterns": [
      "react",
      "node_modules"
    ],
    "modulePathIgnorePatterns": []
  }
}
